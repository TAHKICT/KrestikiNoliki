<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
           version="2.5">


    <servlet>
        <servlet-name>redirectJSPServlet</servlet-name>
        <servlet-class>RunGame.AuthUser</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>redirectJSPServlet</servlet-name>
        <url-pattern>/redirectJSP</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GameController</servlet-name>
        <servlet-class>GameOption.GameController</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GameController</servlet-name>
        <url-pattern>/gamecontroller</url-pattern>
    </servlet-mapping>
</web-app>
